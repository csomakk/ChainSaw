<?xml version="1.0"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009"
                xmlns:s="library://ns.adobe.com/flex/spark"
                toolTip="{ (data as Tag).nameWithoutSpecs() }"
        >
    <fx:Script><![CDATA[
        import csplayer.Classes.Tag;
        ]]></fx:Script>
    <s:Rect x="1" y="1" width="10" height="10">
        <s:fill>
            <s:SolidColor color="{ (data as Tag).color1 }"/>
        </s:fill>
    </s:Rect>
    <s:Rect x="5" y="5" width="2" height="2">
        <s:fill>
            <s:SolidColor color="{ (data as Tag).textColor }"/>
        </s:fill>
    </s:Rect>
</s:ItemRenderer>
