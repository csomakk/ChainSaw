<?xml version="1.0" encoding="utf-8"?>
<Group xmlns:fx="http://ns.adobe.com/mxml/2009"
       xmlns="library://ns.adobe.com/flex/spark"
       width="100%">
    <fx:Script>
		<![CDATA[
        import csplayer.Containers.MainContainer;
        ]]>
	</fx:Script>

    <VGroup width="100%" gap="2">
        <Button width="100%" height="25" label="Library" chromeColor="#E1ECEC"
                click="MainContainer.instance.layoutLibrary2()"
                icon="icons/1342554818_grid.png" skinClass="csplayer.Containers.ButtonSkinOverrided"/>
        <Button width="100%" height="25" label="Facebook" chromeColor="#E1ECEC"
                click="MainContainer.instance.layoutFacebookWall()"
                icon="icons/1342554794_facebook.png" skinClass="csplayer.Containers.ButtonSkinOverrided"/>
        <Button width="100%" height="25" label="YouTube" chromeColor="#E1ECEC"
                click="MainContainer.instance.layoutYoutube()"
                icon="icons/1342554907_youtube_yt.png" skinClass="csplayer.Containers.ButtonSkinOverrided"/>
        <Button width="100%" height="25" label="Folders" chromeColor="#E1ECEC"
                click="MainContainer.instance.layoutFolders()"
                icon="icons/1342555401_folder_personal.png" skinClass="csplayer.Containers.ButtonSkinOverrided"/>
        <Button width="100%" height="25" label="Smart DJ" chromeColor="#E1ECEC"
                icon="icons/1342644027_pin_map_right.png" skinClass="csplayer.Containers.ButtonSkinOverrided"/>
    </VGroup>
</Group>
