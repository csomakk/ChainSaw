<?xml version="1.0"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark">
    <fx:Declarations  >
        <fx:uint id="color1" />
        <fx:uint id="textColor" />
    </fx:Declarations>
    <s:Rect x="1" y="1" width="10" height="10">
        <s:fill>
            <s:SolidColor color="{ color1 }"/>
        </s:fill>
    </s:Rect>
    <s:Rect x="5" y="5" width="2" height="2">
        <s:fill>
            <s:SolidColor color="{ textColor }"/>
        </s:fill>
    </s:Rect>
</s:Group>
